CFLAGS = -g -D__AIS_B_TEST__ +Z

PLATFORM_INCLUDES += 
LDFLAGS := -lpthread
SHLIB_LDFLAGS += -b -o

#64bit CFLAGS LIBS and SAF_OBJDIR vars 
ifeq ($(HPUXARCH), ia64)
  CFLAGS64 = $(CFLAGS) +DD64
else
  CFLAGS += +DAportable +DS2.0a
  CFLAGS64 = $(CFLAGS) +DA2.0W
endif

SAF_OBJDIR64=objs64
FINAL_SAF_OBJDIR64=$(SAF_OBJDIR64)/final
LIBS64=$(IMPLEMENTATION_LIBS64)
