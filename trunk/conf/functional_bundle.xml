  <weight>1</weight>
 </SAFTestCase>

 <SAFTestCase>
  <name>clm_node_get_async_random</name>
  <cmd>clm/operation/saClmClusterNodeGetAsync/nodeGetAsync.rb --node-id=random</cmd>
  <weight>1</weight>
 </SAFTestCase>
</SAFTestCaseList>

<SAFTestCaseList type="final" mode="sequential">
 <SAFTestCase>
  <name>driver_stop_all</name>
  <cmd>driver/stopLongLivedDrivers.rb</cmd>
  <weight/>
 </SAFTestCase>
</SAFTestCaseList>

</SAFTestBundle>
