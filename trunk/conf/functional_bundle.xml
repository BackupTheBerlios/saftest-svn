<?xml version="1.0" encoding="UTF-8"?>
<SAFTestBundle xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="SAFTestBundle.xsd" schemaVersion="1">
<SAFTestCaseList type="initial" mode="sequential">
 <SAFTestCase>
  <name>driver_start_all</name>
  <cmd>driver/startLongLivedDrivers.rb</cmd>
  <weight/>
 </SAFTestCase>

 <SAFTestCase>
  <name>clm_init_all</name>
  <cmd>clm/lifecycle/saClmInitialize/initialize.rb</cmd>
  <weight/>
 </SAFTestCase>

 <SAFTestCase>
  <name>clm_select_obj_get_all</name>
  <cmd>clm/lifecycle/saClmSelectionObjectGet/selectionObjectGet.rb</cmd>
  <weight/>
 </SAFTestCase>
</SAFTestCaseList>

<SAFTestCaseList type="main" mode="random">
