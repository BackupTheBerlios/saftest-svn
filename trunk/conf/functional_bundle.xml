  <name>clm_node_get_local</name>
  <cmd>clm/operation/saClmClusterNodeGet/nodeGet.rb --node-id=local</cmd>
  <weight>1</weight>
 </SAFTestCase>

 <SAFTestCase>
  <name>clm_node_get_random</name>
  <cmd>clm/operation/saClmClusterNodeGet/nodeGet.rb --node-id=random</cmd>
  <weight>1</weight>
